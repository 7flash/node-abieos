{
  "name": "@eosrio/node-abieos",
  "version": "2.1.0-beta.0",
  "description": "Node Bindings for abieos: Binary <> JSON conversion using ABIs.",
  "main": "abieos.js",
  "typings": "abieos.d.ts",
  "scripts": {
    "install": "node -e 'process.exit(0)'",
    "build": "cmake-js compile --cc /usr/bin/clang-9 --cxx /usr/bin/clang++-9 && cp build/Release/node_abieos.node dist/abieos.node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eosrio/node-abieos.git"
  },
  "author": "EOS Rio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eosrio/node-abieos/issues"
  },
  "homepage": "https://github.com/eosrio/node-abieos#readme",
  "devDependencies": {
    "node-addon-api": "2.0.0",
    "cmake-js": "^6.1.0",
    "@types/node": "13.13.1"
  },
  "cmake-js": {
    "runtime": "node",
    "runtimeVersion": "13.0.0",
    "arch": "x64"
  }
}
